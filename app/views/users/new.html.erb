<h2> Join us! </h2>

<form action="<%= users_url %>" method="post">
  <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">

  <label for="user_email">Email </label>
  <input
    name="user[email]"
    id="user_email"
    value="<% @user.email %>"
    >

  <label for="user_password">Password </label>
  <input
    name="user[password]"
    id="user_password"
    value="password"
    >

  <input type="submit">

</form>
